   "source": [
    "n= int(input())\n",
    "pi = list(map(int, input().split()))\n",
    "result = 0\n",
    "pi.sort()\n",
    "\n",
    "for i in range(0,n):\n",
    "    for j in range(0,i+1):\n",
    "        result += pi[j]\n",
    "\n",
    "print(result)"
